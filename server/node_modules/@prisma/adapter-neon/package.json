{
  "name": "@prisma/adapter-neon",
  "version": "6.18.0",
  "description": "Prisma's driver adapter for \"@neondatabase/serverless\"",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
